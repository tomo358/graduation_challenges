<h1>確認画面</h1>
<%= form_with(model: @musical_instrument, url: musical_instruments_path, local: true) do |form| %>
  <%= image_tag(@musical_instrument.image) %>
  <%= form.hidden_field :image_cache %>
  <%= form.submit "投稿する" %>
<% end %>

<%= form_with(model:@musical_instrument, url:new_musical_instruments_path, local:true, method:"get") do |form| %>
  <%= form.hidden_field :image_cache %>
  <%= form.submit "戻る", name: "back" %>
<% end %>
